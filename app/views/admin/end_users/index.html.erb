<div class="container">
	<div class="row">
		<h4>会員一覧画面</h4>
		<div class="col-xs-8">
			<table class="table table-border">
				<thead>
					<tr>
						<th>会員ID</th>
						<th>氏名</th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>
				<tbody>
					<% @end_users.each do |end_user| %>
					<tr>
						<td>
							<%= end_user.id %>
						</td>
						<td>
							<%= end_user.last_name %>
							<%= end_user.first_name %>
						</td>
						<td>
							<%= end_user.email %>
						</td>
						<td>
							<% if end_user.deleted_at.nil? %>
							<span>有効</span>
							<% else %>
							<span>退会済み</span>
							<% end %>
						</td>
					</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>